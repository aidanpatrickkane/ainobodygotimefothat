{
  "manifest_version": 3,
  "author": "Aidan Kane",
  "name": "Ainobodygotimefothat",
  "version": "1.0",
  "permissions": ["activeTab"],
  "host_permissions": [
    "https://api.openai.com/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "aino-logo.webp"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
  